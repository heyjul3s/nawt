{
  "name": "@nawt/media",
  "version": "0.1.0",
  "description": "A media query utility library with support for newer media feature queries.",
  "main": "src/index.ts",
  "keywords": [
    "react",
    "jsx",
    "styled-components",
    "css-in-js",
    "ui",
    "media",
    "media query",
    "layout"
  ],
  "scripts": {
    "test": "jest --passWithNoTests --colors",
    "prettier": "prettier --ignore-path '../../.prettierignore' --check .",
    "prettier:fix": "prettier --ignore-path '../../.prettierignore' --write .",
    "lint:es": "eslint --ignore-pattern '/dist/' . --ext .ts,.tsx .",
    "lint:es-fix": "eslint --ignore-pattern '/dist/' --fix --ext .js,.ts,.tsx .",
    "build": "rimraf dist && bundler"
  },
  "author": "Julian Low <hello@heyjul.es>",
  "license": "MIT",
  "publishConfig": {
