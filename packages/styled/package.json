{
  "name": "@artefakt/styled",
  "version": "0.1.0",
  "description": "A small library of Styled Components utilities",
  "scripts": {
    "test": "jest --passWithNoTests --colors",
    "prettier": "prettier --check . '!dist/*'",
    "prettier:fix": "prettier --write . '!dist/*'",
    "lint:es": "eslint --ignore-pattern '/dist/' . --ext .ts,.tsx .",
    "lint:es-fix": "eslint --ignore-pattern '/dist/' --fix --ext .js,.ts,.tsx .",
    "build": "bundler"
  },
  "author": "Julian Low <hello@heyjul.es>",
  "license": "MIT",
  "publishConfig": {
    "access" : "public",
    "main" : "dist/styled.js",
    "module": "dist/styled.esm.js",
    "typings": "dist/index.d.ts"
  },
  "devDependencies": {
    "@artefakt/bundler": "workspace:0.1.0",
    "@types/styled-components": "^5.1.15",
    "@types/styled-system": "^5.1.13",
    "csstype": "^3.0.10",
    "styled-components": "^5.3.3",
    "styled-system": "^5.1.5"
  },
  "dependencies": {
    "lodash.isempty": "^4.4.0"
  }
}
